IF(INCLUDES( College__r.Exam_Boards__c, &quot;OCR (Oxford, Cambridge and RSA Examinations)&quot;), &quot;OCR (Oxford, Cambridge and RSA Examinations)&quot; + BR() , NULL) +
IF(INCLUDES( College__r.Exam_Boards__c, &quot;WJEC (Welsh Joint Education Committee)	&quot;), &quot;WJEC (Welsh Joint Education Committee)	&quot;, NULL)</formula>
