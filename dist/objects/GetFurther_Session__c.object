        <fullName>Change_session_tutor_not_ticked</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
ISCHANGED( SessionTutor__c ),
 ChangeSessionTutor__c = FALSE,
 ISBLANK(SessionTutor__c)=FALSE )</errorConditionFormula>
        <errorMessage>If you wish to change the session tutor, you must tick the &apos;Change Session Tutor&apos; checkbox</errorMessage>
    </validationRules>
