    <fields>
        <fullName>time_till_session__c</fullName>
        <externalId>false</externalId>
        <formula>FLOOR((Session_Date__c - Now()) * 24) +1</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Time Till Session (Hours)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Session</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Session_Date__c</columns>
        <columns>Subject__c</columns>
        <columns>Tutorial_Topic__c</columns>
        <columns>Tutor__c</columns>
        <columns>TuitionGroup__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <listViews>
        <fullName>Today_s_Sessions</fullName>
        <columns>NAME</columns>
        <columns>TutorEmailPopulated__c</columns>
        <columns>Subject__c</columns>
        <columns>SessionTutor__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>SessionToday__c</field>
            <operation>equals</operation>
            <value>1</value>
        </filters>
        <label>Today&apos;s Sessions</label>
